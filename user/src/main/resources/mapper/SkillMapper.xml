<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jykj.user.mapper.SkillMapper">
<select id="getSkillContentByType" resultType="com.jykj.user.entity.Skill">
    select s.sid,s.skill_content from skill s where s.type=#{type}
</select>
</mapper>
